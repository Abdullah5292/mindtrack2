#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hammaduh/dev/fyp/mindtrack2/material-kit-react-3.0.0/node_modules/is-docker/node_modules:/home/hammaduh/dev/fyp/mindtrack2/material-kit-react-3.0.0/node_modules:/home/hammaduh/dev/fyp/mindtrack2/node_modules:/home/hammaduh/dev/fyp/node_modules:/home/hammaduh/dev/node_modules:/home/hammaduh/node_modules:/home/node_modules:/node_modules:/home/hammaduh/dev/fyp/mindtrack2/material-kit-react-3.0.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hammaduh/dev/fyp/mindtrack2/material-kit-react-3.0.0/node_modules/is-docker/node_modules:/home/hammaduh/dev/fyp/mindtrack2/material-kit-react-3.0.0/node_modules:/home/hammaduh/dev/fyp/mindtrack2/node_modules:/home/hammaduh/dev/fyp/node_modules:/home/hammaduh/dev/node_modules:/home/hammaduh/node_modules:/home/node_modules:/node_modules:/home/hammaduh/dev/fyp/mindtrack2/material-kit-react-3.0.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-docker/cli.js" "$@"
else
  exec node  "$basedir/../is-docker/cli.js" "$@"
fi
